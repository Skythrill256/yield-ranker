{
  "name": "yield-ranker-server",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "start:prod": "npm run build && npm run start",
    "dev": "tsx watch src/index.ts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:reset": "prisma migrate reset",
    "seed:history": "tsx scripts/seed_history.ts",
    "seed:ticker": "tsx scripts/seed_history.ts --ticker",
    "daily:update": "tsx scripts/daily_update.ts",
    "daily:update:force": "tsx scripts/daily_update.ts --force",
    "cron:daily": "tsx scripts/daily_update.ts",
    "cron:daily:force": "tsx scripts/daily_update.ts --force",
    "recalc:metrics": "tsx scripts/recalc_metrics.ts",
    "sync:volatility": "tsx scripts/sync_dividend_volatility.ts",
    "cron:hourly": "tsx scripts/hourly_sync.ts",
    "refresh:all": "tsx scripts/refresh_all.ts && tsx scripts/refresh_cefs.ts",
    "refresh:ticker": "tsx scripts/refresh_all.ts --ticker",
    "refresh:cefs": "tsx scripts/refresh_cefs.ts",
    "refresh:cefs:ticker": "tsx scripts/refresh_cefs.ts --ticker",
    "test:dvi": "tsx scripts/test_dvi_calculation.ts",
    "breakdown:dvi": "tsx scripts/dvi_breakdown.ts",
    "analyze:dvi": "tsx scripts/dvi_analysis.ts",
    "export:dvi": "tsx scripts/dvi_simple_export.ts",
    "tiingo:test": "tsx -e \"import { healthCheck } from './src/services/tiingo.js'; healthCheck().then(console.log)\""
  },
  "dependencies": {
    "@mailerlite/mailerlite-nodejs": "^1.5.0",
    "@prisma/client": "^7.0.1",
    "@supabase/supabase-js": "^2.39.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.8.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "prisma": "^7.0.1",
    "recharts": "^3.5.1",
    "resend": "^6.5.1",
    "typescript": "^5.9.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.0.0",
    "supertest": "^6.3.3",
    "tsx": "^4.7.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}